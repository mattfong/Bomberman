        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_qg9AAnjZEeS6cfjz9k1NoA" name="update" specification="_ppl6MHjZEeS6cfjz9k1NoA">
          <language>java</language>
          <body>gameTimer.update();&#xD;
	for (int i = 0; i &lt; gridWidth; i++) {&#xD;
	    for (int j = 0; j &lt; gridHeight; j++) {&#xD;
		if (!(grid[i][j].empty())) {&#xD;
		    grid[i][j].peek().update();&#xD;
		}&#xD;
	    }&#xD;
	}&#xD;
 </body>
        </ownedBehavior>
        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_qg9nEHjZEeS6cfjz9k1NoA" name="draw" specification="_qar_8XjZEeS6cfjz9k1NoA">
