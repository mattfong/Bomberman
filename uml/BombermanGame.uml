            <defaultValue xmi:type="uml:LiteralString" xmi:id="_qdYHkHjZEeS6cfjz9k1NoA" value="false"/>
          </ownedAttribute>
          <ownedAttribute xmi:id="_qdYHknjZEeS6cfjz9k1NoA" name="leftRight" visibility="private">
            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#Boolean"/>
            <defaultValue xmi:type="uml:LiteralString" xmi:id="_qdYHk3jZEeS6cfjz9k1NoA" value="true"/>
          </ownedAttribute>
