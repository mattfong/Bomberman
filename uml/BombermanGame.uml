          <contents xmi:type="ecore:EAnnotation" xmi:id="_p5uA4XjZEeS6cfjz9k1NoA" source="codegen">
            <details xmi:id="_p5uA4njZEeS6cfjz9k1NoA" key="codegenDirectory" value="/BombermanGame/src"/>
