            <ownedParameter xmi:id="_qhPT4XjZEeS6cfjz9k1NoA" name="actor" type="_ptFTEHjZEeS6cfjz9k1NoA"/>
            <ownedParameter xmi:id="_qhPT4njZEeS6cfjz9k1NoA" name="speed" type="_pr188HjZEeS6cfjz9k1NoA"/>
            <ownedParameter xmi:id="_qhPT43jZEeS6cfjz9k1NoA" type="_pqrfUHjZEeS6cfjz9k1NoA" direction="return"/>
            <language>java</language>
            <body>super(actor, speed);&#xD;
&#xD;
 </body>
