            <language>java</language>
            <body>super(location, world);&#xD;
	sprite = new ImageIcon(Bomberman.class.getResource(&quot;/Sprite.png&quot;));&#xD;
	inputManager = new CommandManager(this, InputListener.getInstance());&#xD;
	bombPass = false;&#xD;
	flamePass = false;&#xD;
	detonator = false;&#xD;
	explosionRadius = 1;&#xD;
	bombLimit = 1;&#xD;
&#xD;
 </body>
