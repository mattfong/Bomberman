            <defaultValue xmi:type="uml:LiteralString" xmi:id="_qeRfcHjZEeS6cfjz9k1NoA" value="null"/>
          </ownedAttribute>
          <ownedAttribute xmi:id="_qeStkHjZEeS6cfjz9k1NoA" name="bufferedKey" visibility="private" isStatic="true" type="_qZBMAXjZEeS6cfjz9k1NoA" association="_qmAfIXjZEeS6cfjz9k1NoA"/>
          <ownedAttribute xmi:id="_qeT7sHjZEeS6cfjz9k1NoA" name="gamePanel" visibility="private" isStatic="true" type="_poREgHjZEeS6cfjz9k1NoA" association="_qmBGMXjZEeS6cfjz9k1NoA"/>
