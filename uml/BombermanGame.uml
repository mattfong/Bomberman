          <language>java</language>
          <body>for (int i = 0; i &lt; gridWidth; i++) {&#xD;
	    for (int j = 0; j &lt; gridHeight; j++) {&#xD;
		if (!grid[i][j].empty()) {&#xD;
		    grid[i][j].peek().draw(g);&#xD;
		}&#xD;
	    }&#xD;
	}&#xD;
 </body>
