        <packagedElement xmi:type="uml:Class" xmi:id="_pzk8oHjZEeS6cfjz9k1NoA" name="InputListener">
          <eAnnotations xmi:id="_pzljsHjZEeS6cfjz9k1NoA" source="http://ns.yatta.de/umllab">
            <contents xmi:type="ecore:EAnnotation" xmi:id="_pzljsXjZEeS6cfjz9k1NoA" source="codegen">
