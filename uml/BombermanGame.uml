          <language>java</language>
          <body>Account acc = null;&#xD;
		for(int i=0; i&lt;numberOfAccounts(); i++){&#xD;
			acc = accounts.get(i);&#xD;
			if(userName.equals(acc.getUserName())){&#xD;
				System.out.println(&quot;Account was found&quot;);&#xD;
				return acc;&#xD;
			}&#xD;
		}&#xD;
		return acc;&#xD;
