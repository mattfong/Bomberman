          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_qidb4HjZEeS6cfjz9k1NoA" name="update" specification="_p0KygHjZEeS6cfjz9k1NoA">
            <language>java</language>
            <body>counter++;&#xD;
	if ((counter % frameRate) == 0) {&#xD;
	    countdown--;&#xD;
	    if (countdown == 0) {&#xD;
		if (world != null) {&#xD;
		    Bomberman bomberman = (Bomberman) world.getBomberman();&#xD;
		    bomberman.die();&#xD;
		}&#xD;
		reset();&#xD;
	    }&#xD;
	    counter = 0;&#xD;
	}&#xD;
 </body>
          </ownedBehavior>
